{
  "manifest_version": 3,
  "name": "Tab Assistant",
  "description": "A chrome extension from help you manage window tabs",
  "permissions": [
    "tabGroups",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": []
    }
  ]
}